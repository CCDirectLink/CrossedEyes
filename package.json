{
    "name": "crossedeyes",
    "version": "0.6.4",
    "scripts": {
        "start": "npx esbuild --target=es2018 --format=esm --platform=node --bundle --sourcemap=inline --outfile=plugin.js src/plugin.ts",
        "watch": "npx esbuild --target=es2018 --format=esm --platform=node --bundle --sourcemap=inline --watch --outfile=plugin.js src/plugin.ts",
        "build": "npx esbuild --target=es2018 --format=esm --platform=node --bundle --outfile=plugin.js --minify-syntax --minify-whitespace src/plugin.ts",
        "format": "prettier ./src -w; prettier -w ./lang; npx prettier ./assets -w --tab-width 4 --no-semi --print-width 500 --bracket-same-line"
    },
    "license": "GPLv3",
    "ccmodHumanName": "CrossedEyes",
    "description": "Accessibility mod for CrossCode",
    "plugin": "plugin.js",
    "ccmodDependencies": {},
    "devDependencies": {
        "@types/jquery": "^3.5.29",
        "@types/node": "^11.6.0",
        "@types/ws": "^8.5.10",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "cc-blitzkrieg": "github:krypciak/cc-blitzkrieg",
        "ccmodmanager": "github:CCDirectLink/CCModManager",
        "crosscode-demonizer": "github:krypciak/crosscode-demonizer",
        "esbuild": "^0.20.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "nax-ccuilib": "github:krypciak/nax-ccuilib",
        "prettier": "3.2.5",
        "typescript": "^5.4.2",
        "ultimate-crosscode-typedefs": "github:krypciak/ultimate-crosscode-typedefs",
        "ws": "^8.16.0"
    }
}
